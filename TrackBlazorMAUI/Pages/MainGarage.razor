@page "/MainGarage"
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/style.css">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.3/font/bootstrap-icons.css">
    <title>My Garage</title>
</head>
<body>
    <div class="container">
        <div class="row justify-content-center">
            <div class="phone_screen_medium col-12 col-sm-12 col-md-12 col-lg-12">
                <div class="garage_header_field col-12 col-sm-12 col-md-12 col-lg-12">
                    <div class="garage_header_bg col-12 col-sm-12 col-md-12 col-lg-12">
                        <div class="garage_header_text_field">
                            <div class="garage_header_name">
                                <p>Hello, Name</p>
                            </div>
                            <div class="garage_header_info">
                                <p>Set your home address</p>
                            </div>
                        </div>
                        <a href="PersonalPage.html" class="navbar_avatar_bg">
                            <div class="navbar_avatar"></div>
                            <div class="navbar_avatar_status"></div>
                        </a>
                    </div>
                </div>

                <div class="garage_group_field">
                    <div class="garage_text_title">
                        <p>Your current Truck is</p>
                    </div>
                    <div class="garage_truck_field">
                        <div class="garage_truck_bg">
                            <div class="garage_big_truck_img"></div>
                        </div>
                    </div>
                    <div class="garage_truck_title">
                        <p>1995 Dodge Ram Van 2500</p>
                    </div>
                    <a href="ExistingGarage.html" class="garage_right_arrow_bg">
                        <div class="garage_right_arrow_logo"></div>
                    </a>
                    <a href="#" class="garage_left_arrow_bg">
                        <div class="garage_left_arrow_logo"></div>
                    </a>
                    <div class="garage_truck_info_field">
                        <div class="garage_truck_length_field">
                            <div class="garage_truck_info_bg">
                                <div class="garage_truck_length_title">
                                    <p>Length</p>
                                </div>
                                <div class="garage_truck_parameters_info">
                                    <p>205.2 "</p>
                                </div>
                            </div>
                        </div>
                        <div class="garage_truck_height_field">
                            <div class="garage_truck_info_bg">
                                <div class="garage_truck_length_title">
                                    <p>Height</p>
                                </div>
                                <div class="garage_truck_parameters_info">
                                    <p>79.0 "</p>
                                </div>
                            </div>
                        </div>
                        <div class="garage_truck_weight_field">
                            <div class="garage_truck_info_bg">
                                <div class="garage_truck_length_title">
                                    <p>Weight</p>
                                </div>
                                <div class="garage_truck_parameters_info">
                                    <p>79.0 "</p>
                                </div>
                            </div>
                        </div>
                        <div class="garage_truck_name_title">
                            <div class="garage_truck_info_bg">
                                <div class="garage_truck_title_info">
                                    <p>1995 Dodge Ram Van 2500</p>
                                </div>
                            </div>
                        </div>
                        <div class="garage_truck_button_length_field">
                            <div class="garage_truck_button_bg">
                                <div class="garage_truck_shuffle_logo"></div>
                            </div>
                        </div>
                        <div class="garage_truck_button_height_field">
                            <div class="garage_truck_button_bg">
                                <div class="garage_truck_shuffle_logo"></div>
                            </div>
                        </div>
                        <div class="garage_truck_button_weight_field">
                            <div class="garage_truck_button_bg">
                                <div class="garage_truck_shuffle_logo"></div>
                            </div>
                        </div>
                        <div class="garage_truck_button_name_title_field">
                            <div class="garage_truck_button_bg">
                                <div class="garage_truck_shuffle_logo"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
